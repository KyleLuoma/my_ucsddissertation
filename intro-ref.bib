author = {Finstad, Kraig},
title = {The Usability Metric for User Experience},
year = {2010},
issue_date = {September, 2010},
publisher = {Elsevier Science Inc.},
address = {USA},
volume = {22},
number = {5},
issn = {0953-5438},
url = {https://doi.org/10.1016/j.intcom.2010.04.004},
doi = {10.1016/j.intcom.2010.04.004},
abstract = {The Usability Metric for User Experience (UMUX) is a four-item Likert scale used for the subjective assessment of an application's perceived usability. It is designed to provide results similar to those obtained with the 10-item System Usability Scale, and is organized around the ISO 9241-11 definition of usability. A pilot version was assembled from candidate items, which was then tested alongside the System Usability Scale during usability testing. It was shown that the two scales correlate well, are reliable, and both align on one underlying usability factor. In addition, the Usability Metric for User Experience is compact enough to serve as a usability module in a broader user experience metric.},
journal = {Interact. Comput.},
month = {sep},
pages = {323–327},
numpages = {5},
keywords = {Metric, Usability, User experience, Scale}
}

